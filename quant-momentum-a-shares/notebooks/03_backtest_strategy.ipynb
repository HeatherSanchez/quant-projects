     "# ğŸ“˜ Aè‚¡åŠ¨é‡ç­–ç•¥å›æµ‹ Notebook\n",
     "\n",
     "# å¯¼å…¥åº“\n",
     "import pandas as pd\n",
     "import numpy as np\n",
     "import matplotlib.pyplot as plt\n",
     "import yfinance as yf\n",
     "\n",
     "# ä¸‹è½½æ•°æ®\n",
     "ticker = \"510300.SS\"\n",
     "df = yf.download(ticker, start=\"2018-01-01\", end=\"2024-12-31\")\n",
     "df = df[['Close']].dropna()\n",
     "\n",
     "# è®¡ç®—åŠ¨é‡\n",
     "window = 20\n",
     "df['momentum'] = df['Close'].pct_change(periods=window)\n",
     "df['signal'] = (df['momentum'] > 0).astype(int)\n",
     "df['ret'] = df['Close'].pct_change()\n",
     "df['strategy_ret'] = df['signal'].shift(1) * df['ret']\n",
     "\n",
     "# ç­–ç•¥èµ„é‡‘æ›²çº¿\n",
     "df['cum_strategy'] = (1 + df['strategy_ret'].fillna(0)).cumprod()\n",
     "df['cum_benchmark'] = (1 + df['ret'].fillna(0)).cumprod()\n",
     "\n",
     "# å¯è§†åŒ–\n",
     "plt.figure(figsize=(10,5))\n",
     "plt.plot(df.index, df['cum_strategy'], label='åŠ¨é‡ç­–ç•¥')\n",
     "plt.plot(df.index, df['cum_benchmark'], label='åŸºå‡†', linestyle='--')\n",
     "plt.legend()\n",
     "plt.title('Aè‚¡åŠ¨é‡ç­–ç•¥å›æµ‹ç»“æœ')\n",
     "plt.show()\n",
     "\n",
     "# æŒ‡æ ‡è®¡ç®—\n",
     "days = df.shape[0]\n",
     "years = days / 252\n",
     "ann_ret = df['cum_strategy'].iloc[-1] ** (1/years) - 1\n",
     "ann_vol = df['strategy_ret'].std() * np.sqrt(252)\n",
     "sharpe = ann_ret / ann_vol\n",
     "print(f\"å¹´åŒ–æ”¶ç›Šç‡: {ann_ret:.2%}\")\n",
     "print(f\"å¹´åŒ–æ³¢åŠ¨ç‡: {ann_vol:.2%}\")\n",
     "print(f\"Sharpe: {sharpe:.2f}\")\n"
